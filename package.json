{
  "name": "eollugage-pc",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "init-project": "yarn install && cd src && yarn install && cd .. && cd electron && yarn install && cd ..",
    "start": "concurrently \"cd src && yarn start\" \"cd electron && yarn electron\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/the-kingdoms/eollugage-pc.git"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "build": {
    "appId": "com.eolluga.eollugage-pc",
    "productName": "얼루가게 PC",
    "files": [
      "build/**/*",
      "icon/**/*"
    ],
    "mac": {
      "target": [
        "default"
      ],
      "icon": "./icon/eollugage/logo.png"
    },
    "dmg": {
      "title": "얼루가게 PC",
      "icon": "./icon/eollugage/logo.png"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./icon/eollugage/logo.png"
    }
  },
  "packageManager": "yarn@1.22.19"
}
